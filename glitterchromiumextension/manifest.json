{
    "name": "Glitter Audit for Sitecore XM cloud",
    "description": "View Sitecore item history and compare versions",
    "version": "0.1",
    "manifest_version": 3,
    "background": {
        "service_worker": "historypopup.js"
    },
    "permissions": ["storage", "tabs", "activeTab", "scripting"],
    "action": {
        "default_popup": "historypopup.html"
    },
    "icons": {
        "16": "icon16.png",
        "32": "icon32.png",
        "48": "icon48.png",
        "128": "icon128.png"
    }
}
